{
  "name": "oleksandr-torosh/yona-cms",
  "version": "1.0.0",
  "description": "Yona CMS - open source content management system (CMS). Written in Phalcon PHP Framework. Has a convenient modular structure. Has simple configuration and architecture. Can be easily modified for any task with any loads.",
  "type": "project",
  "keywords": [
    "phalcon",
    "php",
    "yona",
    "cms",
    "modules",
    "modular",
    "multi-lingual",
    "i18n",
    "l10n"
  ],
  "homepage": "http://yonacms.com/",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Oleksandr Torosh",
      "email": "info@yonastudio.com"
    },
    {
      "name": "Nikolas Serko",
      "email": "nikolas.serko@gmail.com"
    }
  ],
  "maintainers": [
    {
      "name": "Oleksandr Torosh",
      "url": "https://github.com/oleksandr-torosh"
    },
    {
      "name": "Nikolas Serko",
      "url": "https://github.com/SerkoNikolas"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oleksandr-torosh/yona-cms"
  },
  "bugs": "https://github.com/oleksandr-torosh/yona-cms/issues",
  "require": {
    "php": ">=7.2",
    "ext-phalcon": ">=3.3",
    "vlucas/phpdotenv": "^2.4",
    "phalcon/ide-stubs": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "Core\\": "src/core/",
      "Modules\\": "src/modules/",
      "Application\\": "src/app"
    }
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "codeception/codeception": "^2.4"
  }
}
